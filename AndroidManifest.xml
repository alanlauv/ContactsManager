    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="se206.project.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="se206.project.AddEditContactActivity"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name="se206.project.EditGroupsActivity"
            android:label="@string/title_activity_edit_groups" >
        </activity>
        <activity
            android:name="se206.project.ViewContactActivity"
            android:label="@string/title_activity_view_contact" >
        </activity>
    </application>

</manifest>
